        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [
            {
              "options": {
                "0": {
                  "color": "text",
                  "index": 0,
                  "text": "Unknown"
                },
                "1": {
                  "color": "green",
                  "index": 1,
                  "text": "Healthy"
                },
                "2": {
                  "color": "yellow",
                  "index": 2,
                  "text": "Degraded"
                },
                "3": {
                  "color": "red",
                  "index": 3,
                  "text": "Failing"
                }
              },
              "type": "value"
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "text",
                "value": null
              },
              {
                "color": "green",
                "value": 1
              },
              {
                "color": "yellow",
                "value": 2
