# Service Configuration
EXAMPLE_SERVICE_SERVICE_NAME=example-service
EXAMPLE_SERVICE_SERVICE_PORT=8000
EXAMPLE_SERVICE_DEBUG=false
EXAMPLE_SERVICE_LOG_LEVEL=INFO

# Database Configuration
EXAMPLE_SERVICE_DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/example_service
EXAMPLE_SERVICE_DATABASE_POOL_SIZE=20
EXAMPLE_SERVICE_DATABASE_MAX_OVERFLOW=10

# Cache Configuration
EXAMPLE_SERVICE_REDIS_URL=redis://localhost:6379/0
EXAMPLE_SERVICE_CACHE_TTL=3600

# External Services (Optional)
# EXAMPLE_SERVICE_AUTH_SERVICE_URL=http://localhost:8001
# EXAMPLE_SERVICE_CONFD_SERVICE_URL=http://localhost:8002

# Observability
EXAMPLE_SERVICE_ENABLE_METRICS=true
EXAMPLE_SERVICE_ENABLE_TRACING=false
